<p>会員情報編集</p>

<%= form_with model: @customer, url:customer_path, method: :patch do |f| %>

 <div>
   <%= f.label :"氏名" %>
   <%= f.text_field :family_name %>
   <%= f.text_field :first_name %>
 </div>
 
 <div>
   <%= f.label :"フリガナ" %>
   <%= f.text_field :family_name_kana %>
   <%= f.text_field :first_name_kana %>
 </div>
 
 <div>
   <%= f.label :"郵便番号" %>
   <%= f.text_field :post_code %>
 </div>
 
 <div>
   <%= f.label :"住所" %>
   <%= f.text_field :address %>
 </div>
 
 <div>
   <%= f.label :"電話番号" %>
   <%= f.text_field :telephone_number %>
 </div>
 
 <div>
   <%= f.label :"メールアドレス" %>
   <%= f.text_field :email %>
 </div>
 
 <%= f.submit "編集内容を保存" %>
 
 <!--%= link_to "退会する", customer_customers_leave_path %>
<% end %>